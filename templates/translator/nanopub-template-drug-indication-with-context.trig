    nt:hasStatement :st00 , :st01, :st02 , :st03 , :st04 , :st05, :st06, :st07, 
      :st08, :st09, :st10, :st11, :st12, :st13, :st14 .

  :associationRelation a nt:GuidedChoicePlaceholder ;
    rdfs:label "provide the type of drug indication" ;
    nt:possibleValue schema:TreatmentIndication, schema:PreventionIndication, 
